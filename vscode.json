{
  "scss.validate": false,
  "css.validate": false,
  "eslint.validate": [
    "vue",
    "html",
    "javascript"
  ],
  "rules": {
    "at-rule-no-unknown": [
      true,
      {
        "ignoreAtRules": [
          "tailwind",
          "apply",
          "variants",
          "responsive",
          "screen",
        ],
      },
    ],
  }
}